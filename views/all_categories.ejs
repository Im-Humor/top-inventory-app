<%- include("header"); -%>

<div class="main-content">
    <p>These are the available categories:</p>
    <ul>
    <% categories.forEach((item) => { %>
        <li>
            <p>
                <%= item.name %>
            </p>
            <p>
                <%=item.desc %>
            </p>
            <p>
                <a href="<%= item.url %>">View Category</a>
            </p>
        </li>
        <% }) %>
    </ul>

    <a href="/catalog/categories/new">Add New Category</a>

</div>

        
<%- include("footer"); -%>